{
  "title": "Trantrace Documentation",
  "plugins": [
    "fancybox", "-highlight", "prism", "prism-themes", "callouts", "terminal",
    "emphasize", "splitter", "simple-page-toc", "katex"
  ],
  "pluginsConfig": {
    "sharing": {
      "facebook": true,
      "google": true,
      "twitter": true,
      "weibo": true,
      "all": ["facebook", "google", "twitter", "weibo"]
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-darcula.css"
      ]
    },
    "callouts": {
      "showTypeInHeader": false
    }
  }
}
